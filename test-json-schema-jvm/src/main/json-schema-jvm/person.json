{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Person",
  "type": "object",
  "additionalProperties": false,
  "properties": {
    "name": {
      "type": "string"
    },
    "age": {
      "type": "integer"
    },
    "date": {
      "type": "array",
      "items": {
        "type": "integer"
      }
    },
    "time": {
      "type": "array",
      "items": {
        "type": "integer"
      }
    },
    "dateTime": {
      "type": "array",
      "items": {
        "type": "integer"
      }
    },
    "instant": {
      "type": "number"
    }
  },
  "required": [
    "age"
  ]
}
